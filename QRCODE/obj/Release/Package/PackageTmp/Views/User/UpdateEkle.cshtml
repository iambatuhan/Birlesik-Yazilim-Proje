@model EntityLayer.Concrete.User
@{
    ViewBag.Title = "UpdateEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@using (Html.BeginForm("UpdateEkle", "User", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.Label("Kişinin ID'si")
    @Html.TextBoxFor(x => x.UserID, new { @class = "form-control" })
    <br />
    @Html.Label("Kişinin Tc'si")
    @Html.TextBoxFor(x => x.UserTc, new { @class = "form-control" })
    <br />
    @Html.Label("Kişinin Adı")
    @Html.TextBoxFor(x => x.UserName, new { @class = "form-control" })
    <br />
   
    @Html.Label("Kişinin Maili")
    @Html.TextBoxFor(x => x.Mail, new { @class = "form-control" })

    <br />
    @Html.Label("Kişinin Mesleği")
    @Html.TextBoxFor(x => x.Meslek, new { @class = "form-control" })
    <br />
    <br />
    <br />
    <button class="btn btn-outline-primary">Kaydet</button>
}


